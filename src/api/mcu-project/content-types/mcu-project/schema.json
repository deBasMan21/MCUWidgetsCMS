{
  "kind": "collectionType",
  "collectionName": "mcu_projects",
  "info": {
    "singularName": "mcu-project",
    "pluralName": "mcu-projects",
    "displayName": "MCUProject",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "ReleaseDate": {
      "type": "date",
      "required": false
    },
    "directors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::director.director",
      "inversedBy": "mcu_projects"
    },
    "actors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::actor.actor",
      "inversedBy": "mcu_projects"
    },
    "PostCreditScenes": {
      "type": "integer"
    },
    "Duration": {
      "type": "integer"
    },
    "Phase": {
      "type": "enumeration",
      "enum": [
        "Phase 1",
        "Phase 2",
        "Phase 3",
        "Phase 4",
        "Phase 5",
        "Phase 6",
        "Unkown"
      ],
      "required": true
    },
    "Saga": {
      "type": "enumeration",
      "enum": [
        "Infinity Saga",
        "Multiverse Saga",
        "Unkown"
      ],
      "required": true
    },
    "Overview": {
      "type": "richtext",
      "required": false
    },
    "related_projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mcu-project.mcu-project"
    },
    "Trailers": {
      "type": "component",
      "repeatable": true,
      "component": "trailer.trailer",
      "required": false
    },
    "Posters": {
      "type": "component",
      "repeatable": true,
      "component": "poster.poster",
      "min": 1,
      "required": true,
      "max": 10
    },
    "Seasons": {
      "type": "component",
      "repeatable": true,
      "component": "episode.season",
      "required": false
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Serie",
        "Movie",
        "Special"
      ],
      "required": true
    },
    "BoxOffice": {
      "type": "biginteger",
      "min": "0"
    },
    "DisneyPlusUrl": {
      "type": "string"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::strapi-plugin-fcm.fcm-notification",
      "mappedBy": "mcu_project"
    },
    "imdb_id": {
      "type": "string",
      "required": false,
      "unique": true,
      "private": true
    },
    "Rating": {
      "type": "decimal"
    },
    "VoteCount": {
      "type": "integer"
    },
    "Categories": {
      "type": "string"
    },
    "AwardsNominated": {
      "type": "integer"
    },
    "AwardsWon": {
      "type": "integer"
    },
    "Quote": {
      "type": "string"
    },
    "ProductionBudget": {
      "type": "integer"
    },
    "QuoteCaption": {
      "type": "string"
    },
    "reviewTitle": {
      "type": "string"
    },
    "reviewSummary": {
      "type": "string"
    },
    "reviewCopyright": {
      "type": "string"
    },
    "ReleaseDateNameOverride": {
      "type": "string"
    },
    "rankingCurrentRank": {
      "type": "integer"
    },
    "rankingDifference": {
      "type": "integer"
    },
    "rankingChangeDirection": {
      "type": "enumeration",
      "enum": [
        "UP",
        "DOWN",
        "FLAT"
      ]
    }
  }
}
