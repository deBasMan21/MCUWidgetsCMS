{
  "kind": "collectionType",
  "collectionName": "related_projects",
  "info": {
    "singularName": "related-project",
    "pluralName": "related-projects",
    "displayName": "RelatedProject",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "ReleaseDate": {
      "type": "date"
    },
    "Posters": {
      "type": "component",
      "repeatable": true,
      "component": "poster.poster",
      "required": true,
      "min": 1,
      "max": 10
    },
    "Trailers": {
      "type": "component",
      "repeatable": true,
      "component": "trailer.trailer",
      "required": false
    },
    "Seasons": {
      "type": "component",
      "repeatable": true,
      "component": "episode.season"
    },
    "Overview": {
      "type": "richtext",
      "required": false
    },
    "actors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::actor.actor",
      "inversedBy": "related_projects"
    },
    "directors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::director.director",
      "inversedBy": "related_projects"
    },
    "WatchLink": {
      "type": "string"
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Fox",
        "MarvelTelevision",
        "MarvelOther",
        "Sony",
        "Defenders"
      ],
      "required": true
    }
  }
}
